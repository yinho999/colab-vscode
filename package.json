{
  "name": "colab",
  "publisher": "google",
  "displayName": "Colab",
  "description": "Connect to Colab runtimes.",
  "version": "0.0.1",
  "type": "commonjs",
  "engines": {
    "vscode": "^1.93.1"
  },
  "extensionDependencies": [
    "ms-toolsai.jupyter"
  ],
  "categories": [
    "Notebooks"
  ],
  "activationEvents": [
    "onNotebook:jupyter-notebook",
    "onNotebook:interactive"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Colab",
      "properties": {
        "colab.resourceProxyBaseUrl": {
          "type": "string",
          "default": "",
          "description": "A temporary setting to hardcode a resource proxy base URL."
        },
        "colab.resourceProxyToken": {
          "type": "string",
          "default": "",
          "description": "A temporary setting to hardcode a resource proxy token."
        }
      }
    }
  },
  "scripts": {
    "clean": "rm -rf out/*",
    "compile": "tsc -p ./",
    "lint:fix": "eslint src/ --fix",
    "lint": "eslint src/",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "pretest": "npm run compile && npm run lint",
    "test:unit": "npm run compile && mocha --enable-source-maps --require ./out/test/unit-test-setup.js './out/**/*.unit.test.js'",
    "test:xvfb": "xvfb-run -s '-screen 0 1024x768x24' node ./out/test/integration-test-runner.js",
    "test": "node ./out/test/integration-test-runner.js",
    "vscode:prepublish": "npm run compile",
    "watch:prod": "tsc -watch -p ./tsconfig.prod.json",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@cspell/eslint-plugin": "^8.17.3",
    "@eslint/js": "^9.15.0",
    "@types/chai": "^4.3.20",
    "@types/chai-as-promised": "^7.1.8",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/node-fetch": "^2.6.12",
    "@types/sinon": "^17.0.3",
    "@types/uuid": "^10.0.0",
    "@types/vscode": "^1.93.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "chai": "^4.5.0",
    "chai-as-promised": "^7.1.2",
    "cspell": "^8.17.3",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "mocha": "^10.8.2",
    "prettier": "3.4.2",
    "sinon": "^19.0.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.15.0"
  },
  "dependencies": {
    "@vscode/jupyter-extension": "1.0.93",
    "glob": "^11.0.0",
    "google-auth-library": "^9.15.0",
    "node-fetch": "^2.6.7",
    "uuid": "^11.0.3"
  }
}
