{
  "name": "colab",
  "publisher": "google",
  "displayName": "Colab",
  "description": "Connect to Colab runtimes.",
  "version": "0.0.1",
  "repository": "googlecolab/colab-vscode",
  "type": "commonjs",
  "engines": {
    "vscode": "^1.100.2"
  },
  "extensionDependencies": [
    "ms-toolsai.jupyter"
  ],
  "categories": [
    "Notebooks"
  ],
  "activationEvents": [
    "onNotebook:jupyter-notebook",
    "onNotebook:interactive"
  ],
  "contributes": {
    "authentication": [
      {
        "label": "Google",
        "id": "google"
      }
    ],
    "commands": [
      {
        "category": "Colab",
        "command": "colab.renameServerAlias",
        "enablement": "colab.hasAssignedServer",
        "title": "Rename Server Alias"
      },
      {
        "category": "Colab",
        "command": "colab.removeServer",
        "enablement": "colab.hasAssignedServer",
        "title": "Remove Server"
      },
      {
        "command": "colab.toolbarCommand",
        "category": "Colab",
        "title": "Colab",
        "icon": {
          "light": "resources/colab_grayscale_light.svg",
          "dark": "resources/colab_grayscale_dark.svg"
        }
      }
    ],
    "menus": {
      "notebook/toolbar": [
        {
          "command": "colab.toolbarCommand",
          "group": "navigation/control"
        }
      ],
      "commandPalette": [
        {
          "command": "colab.toolbarCommand",
          "when": "false"
        }
      ]
    }
  },
  "main": "./out/extension.js",
  "scripts": {
    "clean": "rm -rf out/*",
    "generate:config": "tsx ./scripts/generate-config.mts",
    "build": "concurrently \"npm:build:extension\" \"npm:build:tests\" \"npm:typecheck\" --prefixColors=auto",
    "build:extension": "node esbuild.config.mjs",
    "build:tests": "node esbuild.config.mjs --tests",
    "typecheck": "tsc --noEmit",
    "typecheck:prod": "npm run typecheck -- -p tsconfig.prod.json",
    "watch": "concurrently \"npm:watch:extension\" \"npm:watch:tests\" \"npm:watch:typecheck\" --prefixColors=auto",
    "watch:prod": "concurrently \"npm:watch:extension\" \"npm:watch:typecheck:prod\" --prefixColors=auto",
    "watch:extension": "npm run build:extension -- --watch",
    "watch:tests": "npm run build:tests -- --watch",
    "watch:typecheck": "npm run typecheck -- --watch --preserveWatchOutput",
    "watch:typecheck:prod": "npm run typecheck:prod -- --watch --preserveWatchOutput",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "pretest": "npm run build && npm run lint",
    "test:unit": "npm run build:tests && mocha --enable-source-maps --require ./out/test/test/unit-test-setup.js './out/test/**/*.unit.test.js'",
    "test:integration": "npm run build && node ./out/test/test/integration-test-runner.js",
    "test:integration:headless": "xvfb-run -s '-screen 0 1024x768x24' npm run test:integration",
    "pretest:e2e": "npm run build",
    "test:e2e": "extest setup-and-run ./out/test/**/*.e2e.test.js",
    "vscode:prepublish": "npm run package",
    "package": "concurrently \"npm:typecheck:prod\" \"npm:build:extension -- --production\" --prefixColors=auto"
  },
  "devDependencies": {
    "@cspell/eslint-plugin": "^8.17.3",
    "@eslint/js": "^9.15.0",
    "@stylistic/eslint-plugin-ts": "^4.0.1",
    "@types/chai": "^4.3.20",
    "@types/chai-as-promised": "^7.1.8",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/node-fetch": "^2.6.12",
    "@types/sinon": "^17.0.3",
    "@types/uuid": "^10.0.0",
    "@types/vscode": "^1.100.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "chai": "^4.5.0",
    "chai-as-promised": "^7.1.2",
    "clipboardy": "^4.0.0",
    "concurrently": "^9.1.2",
    "cspell": "^8.17.3",
    "dotenv": "^16.5.0",
    "esbuild": "^0.25.2",
    "esbuild-node-externals": "^1.18.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-check-file": "^3.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-tsdoc": "^0.4.0",
    "mocha": "^10.8.2",
    "prettier": "3.4.2",
    "sinon": "^19.0.2",
    "tsx": "^4.19.4",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.15.0",
    "vscode-extension-tester": "^8.14.1"
  },
  "dependencies": {
    "@vscode/jupyter-extension": "1.0.93",
    "glob": "^11.0.0",
    "google-auth-library": "^9.15.0",
    "node-fetch": "^2.6.7",
    "uuid": "^11.0.3",
    "zod": "^3.24.2"
  }
}
