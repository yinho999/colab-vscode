{
  "name": "colab",
  "publisher": "google",
  "displayName": "Colab",
  "description": "Connect to Colab runtimes.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.1"
  },
  "categories": [
    "Notebooks"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "colab.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "clean": "rm -rf out/*",
    "compile": "tsc -p ./",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "pretest": "npm run compile && npm run lint",
    "test:unittests": "npm run compile && mocha --enable-source-maps './out/**/*.unit.test.js'",
    "test:xvfb": "xvfb-run -s '-screen 0 1024x768x24' node ./out/test/runTest.js",
    "test": "node ./out/test/runTest.js",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/chai": "^4.3.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/sinon": "^17.0.3",
    "@types/vscode": "^1.93.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "chai": "^4.5.0",
    "eslint": "^9.15.0",
    "eslint-plugin-import": "^2.31.0",
    "mocha": "^10.8.2",
    "sinon": "^19.0.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.15.0"
  },
  "dependencies": {
    "@vscode/jupyter-extension": "^1.1.1",
    "glob": "^11.0.0"
  }
}
